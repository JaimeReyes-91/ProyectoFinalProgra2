	<parameter name="factura_id" class="java.lang.Long"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    f.factura_id,
    f.numero_serie,
    f.fecha,
    c.nombre AS nombre_cliente,
    c.telefono AS telefono_cliente,
    c.correo AS correo_cliente,
    c.direccion AS direccion_cliente,
    c.nit AS nit_cliente,
    p.producto_id,
    p.nombre_producto,
    d.cantidad,
    d.precio_unitario,
    d.importe,
    f.subtotal,
    f.descuento,
    f.iva,
    f.total,
    f.forma_pago
FROM facturas f
JOIN clientes c ON f.cliente_id = c.cliente_id
JOIN detalle_factura d ON f.factura_id = d.factura_id
JOIN productos p ON d.producto_id = p.producto_id
WHERE f.factura_id = $P{factura_id} ]]>
	</queryString>
